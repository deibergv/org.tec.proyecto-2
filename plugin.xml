<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="org.tec.proyecto2.fragment1"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="notexists"
            uri="fragment1.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Step In"
            id="stepIn.commands.category">
      </category>
      <command
            name="Step In"
            categoryId="stepIn.commands.category"
            id="stepIn.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="stepIn.commands.sampleCommand"
            class="org.tec.proyecto2.flowchart.handlers.StepInHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="stepIn.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
    	 point="org.eclipse.ui.menus">
	      <menuContribution
	            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
	         <toolbar
	               id="stepIn.toolbars.sampleToolbar">
	            <command
	                  commandId="stepIn.commands.sampleCommand"
	                  icon="icons/stepIn.png"
	                  tooltip="Step In"
	                  id="stepIn.toolbars.sampleCommand">
	            </command>
	         </toolbar>
	      </menuContribution>
   </extension>
</plugin>
